{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/ui/floating-dock.tsx", "src/app/(root)/Brand/newpost/page.jsx", "src/app/Brand/post/[postId]/page.tsx", "src/app/Brand/profile/page.tsx", "src/app/Creator/page.jsx", "src/app/Creator/post/[postId]/page.jsx", "src/app/Creator/profile/page.tjsx", "src/components/Navbar.tsx"],
  "exclude": ["node_modules"]
}
